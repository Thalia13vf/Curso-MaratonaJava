package br.com.abc.javacore.associacao.classes.exercicio;

public class Seminario 
{
	private String titulo;
	private Aluno [] alunos;
	private Professor professor; //um seminario só poderá ser ministrado por 1 prof
	private Local local;
	
	public Seminario() {}
	public Seminario(String titulo, Aluno[] alunos, Professor professor, Local local) 
	{
		this.titulo = titulo;
		this.alunos = alunos;
		this.professor = professor;
		this.local = local;
	}
	
	public void listaSeminarios(Seminario seminario, Aluno[] alunos, Professor professor) 
	{
		System.out.println("Título do Seminário: " + seminario.titulo);
		System.out.println("Alunos Incritos: " );
		for(Aluno aluno: alunos) 
		{
			System.out.print(aluno + " ");
		}
		System.out.println("\n" + " ");
	}
	
	public String getTitulo() {
		return titulo;
	}
	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}
	public Aluno[] getAlunos() {
		return alunos;
	}
	public void setAlunos(Aluno[] alunos) {
		this.alunos = alunos;
	}
	public Professor getProfessor() {
		return professor;
	}
	public void setProfessor(Professor professor) {
		this.professor = professor;
	}
	
	
	
	
	
}
